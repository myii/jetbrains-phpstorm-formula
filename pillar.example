phpstorm:
  dl:
    retries: 1
    interval: 30
  linux:
    #Enable Debian alternatives feature by setting nonzero 'altpriority' value here.
    #Increase same value on each subsequent software installation.
    altpriority: 0
  prefs:
    user: undefined_user
    #group:
    #See https://www.jetbrains.com/help/phpstorm/exporting-and-importing-settings.html
    jarurl:
    jardir:
    jarfile: phpstorm-settings.jar
 
